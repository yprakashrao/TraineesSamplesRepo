<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xmlns:p="http://www.springframework.org/schema/p"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
   
    <!-- A simple bean definition -->
   <bean name="cus" class = "com.yprs.dependencyinjection.constructorinjection.Customer">
   <property name="name" value="prakash"></property>
   <property name="contact" value="1234"></property>
   <property name="address">
  <props>
  <prop key="home">vizag</prop>
  <prop key="office">hyd</prop>
  <prop key="office2">chennai</prop>
  </props>
   </property>
   </bean>
  
   
   
      <!-- collaborators and configuration for this bean go here -->
  
   
   </beans>